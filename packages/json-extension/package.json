{
  "name": "@jupyterlab/json-extension",
  "version": "4.0.4",
  "description": "JupyterLab - JSON Renderer",
  "homepage": "https://github.com/jupyterlab/jupyterlab",
  "bugs": {
    "url": "https://github.com/jupyterlab/jupyterlab/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jupyterlab/jupyterlab.git"
  },
  "license": "BSD-3-Clause",
  "author": "Project Jupyter",
  "sideEffects": [
    "style/*"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "style": "style/index.css",
  "directories": {
    "lib": "lib/"
  },
  "files": [
    "lib/*.{d.ts,js,js.map}",
    "style/*",
    "src/**/*.{ts,tsx}"
  ],
  "scripts": {
    "build": "tsc -b",
    "clean": "rimraf lib && rimraf tsconfig.tsbuildinfo",
    "docs": "typedoc src",
    "watch": "tsc -b --watch"
  },
  "dependencies": {
    "@jupyterlab/apputils": "^4.1.4",
    "@jupyterlab/codemirror": "^4.0.4",
    "@jupyterlab/rendermime-interfaces": "^3.8.4",
    "@jupyterlab/translation": "^4.0.4",
    "@jupyterlab/ui-components": "^4.0.4",
    "@lezer/highlight": "^1.1.4",
    "@lumino/coreutils": "^2.1.1",
    "@lumino/messaging": "^2.0.0",
    "@lumino/widgets": "^2.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-highlight-words": "^0.20.0",
    "react-json-tree": "^0.18.0",
    "style-mod": "^4.0.0"
  },
  "devDependencies": {
    "@types/react": "^18.0.26",
    "@types/react-dom": "^18.0.9",
    "@types/react-highlight-words": "^0.16.4",
    "rimraf": "~3.0.0",
    "typedoc": "~0.24.7",
    "typescript": "~5.0.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "jupyterlab": {
    "mimeExtension": true
  },
  "styleModule": "style/index.js"
}
